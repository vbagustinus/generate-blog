<%- include('assets/partials/dashboard-header',{my_title:'Dashboard - All Post',folder:'../../../'})%>

  <!--main content start-->
  <section id="main-content">
    <section class="wrapper">
      <div style="" class="row">
        <div class="col-lg-12">
          <section class="panel">
            <header class="panel-heading">
              Setting Profile
            </header>
            <div class="panel-body">
              <div class="position-center">
                <form action="/<%=session%>/user/setting/<%=user_id%>" role="form-horizontal" method="post">
                  <div class="form-group">
                    <label for="exampleInputEmail1">First Name</label>
                    <input name="first_name" type="text" class="form-control" value="<%=data_User.first_name%>">
                  </div>
                  <div class="form-group">
                    <label for="exampleInputEmail1">Last Name</label>
                    <input name="last_name" type="text" class="form-control" value="<%=data_User.last_name%>">
                  </div>
                  <div class="form-group">
                    <label for="exampleInputPassword1">User Name</label>
                    <input name="username" type="text" class="form-control" value="<%=data_User.username%>">
                  </div>
                  <div class="form-group">
                    <label for="exampleInputPassword1">Blog Name</label>
                    <input name="blog_name"type="text" class="form-control" value="<%=data_User.blog_name%>">
                  </div>
                  <div class="form-group">
                    <label for="exampleInputPassword1">Email Name</label>
                    <input name="email" type="text" class="form-control" value="<%=data_User.email%>">
                  </div>
                  <div class="form-group">
                    <label for="exampleInputPassword1">Gender</label>
                    <select name="gender" id="e9" style="width:300px" class="populate select2-offscreen" tabindex="-1" value="">
                      <option value="female"
                      <% if(data_User.gender == 'female'){ %>
                        selected
                      <%}%>
                      >Female</option>
                      <option value="male"
                      <% if(data_User.gender == 'male'){ %>
                        selected
                      <%}%>
                      >Male</option>
                    </select>
                  </div>

                  <button type="submit" class="btn btn-info">Update</button>
                </form>
              </div>

            </div>
          </section>
    </section>
    </div>
    </div>
  </section>
  <!--main content end-->

  <%- include('assets/partials/dashboard-footer',{folder:'../../../'})%>
